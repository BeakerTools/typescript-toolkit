{
  "name": "@beaker-tools/typescript-toolkit",
  "version": "0.0.0",
  "description": "Typescript toolkit for Scrypto integrators.",
  "keywords": [
    "radix",
    "beaker",
    "typescript",
    "toolkit",
    "dApp",
    "web3"
  ],
  "authors": [
    "Arthur Vinciguerra <arthur@beaker.fi>"
  ],
  "license": "MIT",
  "scripts": {
    "dev": "vite serve examples",
    "build": "tsc && vite build && vite build --config vite-single-file.config.ts",
    "test": "jest"
  },
  "main": "dist/typescript-toolkit.mjs",
  "exports": {
    ".": {
      "import": {
        "types": "./types/index.d.ts",
        "default": "./dist/typescript-toolkit.mjs"
      },
      "require": {
        "types": "./types/index.d.ts",
        "default": "./dist/typescript-toolkit.umd.js"
      }
    }
  },
  "types": "./types/index.d.ts",
  "files": [
    "dist",
    "types"
  ],
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "jest": "^29.7.0",
    "prettier": "3.2.5",
    "semantic-release": "^23.0.8",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@radixdlt/babylon-gateway-api-sdk": "^1.2.7",
    "@radixdlt/radix-engine-toolkit": "^1.0.3",
    "bignumber.js": "^9.1.2",
    "p-limit": "^3.1.0",
    "vite": "^5.2.9",
    "vite-plugin-singlefile": "^2.0.1"
  },
  "release": {
    "branches": [
      "main",
      "next",
      {
        "name": "develop",
        "channel": "dev",
        "prerelease": "dev"
      },
      {
        "name": "release/([a-z0-9-.]+)",
        "channel": ",${name.replace(/^release\\//g, \"\")}",
        "prerelease": "${name.replace(/^release\\//g, \"\")}"
      }
    ]
  }
}
